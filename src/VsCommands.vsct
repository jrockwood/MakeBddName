<?xml version="1.0" encoding="utf-8"?>
<CommandTable xmlns="http://schemas.microsoft.com/VisualStudio/2005-10-18/CommandTable" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <Extern href="stdidcmd.h" />
  <Extern href="vsshlids.h" />
  <Include href="KnownImageIds.vsct" />

  <Commands package="guidPackage">
    <Groups>
      <Group guid="guidTopMenu" id="TopMenu" priority="0x0001" />
    </Groups>

    <Buttons>
      <Button guid="guidMakeBddNameCmdSet" id="cmdMakeBddName" priority="0x0100" type="Button">
        <Parent guid="guidTopMenu" id="TopMenu" />
        <Icon guid="ImageCatalogGuid" id="ConvertToCodeWebTest" />
        <CommandFlag>DynamicVisibility</CommandFlag>
        <CommandFlag>IconAndText</CommandFlag>
        <CommandFlag>IconIsMoniker</CommandFlag>
        <Strings>
          <ButtonText>Make BDD Name</ButtonText>
        </Strings>
      </Button>
    </Buttons>
  </Commands>

  <CommandPlacements>
    <CommandPlacement guid="guidTopMenu" id="TopMenu" priority="0x0c00">
      <Parent guid="guidSHLMainMenu" id="IDM_VS_MENU_EDIT" />
    </CommandPlacement>
    <CommandPlacement guid="guidTopMenu" id="TopMenu" priority="0x0150">
      <Parent guid="guidSHLMainMenu" id="IDM_VS_CTXT_CODEWIN" />
    </CommandPlacement>
  </CommandPlacements>

  <VisibilityConstraints>
    <VisibilityItem guid="guidMakeBddNameCmdSet" id="cmdMakeBddName" context="GUID_TextEditorFactory" />
  </VisibilityConstraints>

  <KeyBindings>
    <!-- VK_OEM_7 is the single/double quote on the US standard keyboard -->
    <KeyBinding guid="guidMakeBddNameCmdSet" id="cmdMakeBddName" mod1="Control" key1="VK_OEM_7"
                mod2="Control" key2="VK_OEM_7" editor="GUID_TextEditorFactory" />
  </KeyBindings>

  <Symbols>
    <!-- This is the package guid. -->
    <GuidSymbol name="guidPackage" value="{c6f71eb1-1c7f-4158-877b-7d4b8e6dc5cc}" />

    <GuidSymbol name="guidTopMenu" value="{98aed3f7-0463-4de4-91fd-1e66a0975e58}">
      <IDSymbol name="TopMenu" value="0x3001" />
    </GuidSymbol>

    <GuidSymbol name="guidMakeBddNameCmdSet" value="{939ce9d0-7724-4468-95c8-d073c7d5ea9e}">
      <IDSymbol name="cmdMakeBddName" value="0x0100" />
    </GuidSymbol>
  </Symbols>
</CommandTable>
